{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes":"off",
        "no-underscore-dangle":0,
        "arrow-body-style": 0,
        "no-shadow": 0,
        "consistent-return": 0,
        "no-nested-ternary": 0,
        "no-console": 1,
        "indent": "off",
        "linebreak-style":"off",
        "no-fallthrough":"off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent":"off"
    }
}
