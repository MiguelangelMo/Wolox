{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "eslint:standard",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes":"off",
        "no-underscore-dangle":0,
        "arrow-body-style": 0,
        "no-shadow": 0,
        "consistent-return": 0,
        "no-nested-ternary": 0,
        "no-console": 1,
        "indent": "off",
        "linebreak-style":"off",
        "no-fallthrough":"off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent":"off"
    }
}
